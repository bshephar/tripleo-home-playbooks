CADeployment:
  config: |
    #!/bin/sh
    cat > ${cacert_path} << EOF
    ${cacert_content}
    EOF
    chmod 0444 ${cacert_path}
    chown root:root ${cacert_path}
    ${update_anchor_command}
    md5sum ${cacert_path} > ${heat_outputs_path}.root_cert_md5sum

  creation_time: "2022-09-12T05:05:28Z"
  deployment_name: CADeployment
